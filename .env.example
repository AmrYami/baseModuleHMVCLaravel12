APP_NAME=Laravel
APP_ENV=local
APP_KEY=
APP_DEBUG=true
APP_TIMEZONE=UTC
APP_URL=http://localhost/php8.4/base_module_v1/public

APP_LOCALE=en
APP_FALLBACK_LOCALE=en
APP_FAKER_LOCALE=en_US

APP_MAINTENANCE_DRIVER=file
# APP_MAINTENANCE_STORE=database

PHP_CLI_SERVER_WORKERS=4

BCRYPT_ROUNDS=12

LOG_CHANNEL=stack
LOG_STACK=single
LOG_DEPRECATIONS_CHANNEL=null
LOG_LEVEL=debug
CSP_REPORT_ONLY=false
CSP_REPORT_URI=/php8.4/base_module_v1/public/api/csp-report

DB_CONNECTION=mysql
DB_HOST=127.0.0.1
DB_PORT=3306
DB_DATABASE=fakeeh
DB_USERNAME=root
DB_PASSWORD=

SESSION_DRIVER=database
SESSION_LIFETIME=120
SESSION_ENCRYPT=false
SESSION_PATH=/
SESSION_DOMAIN=null
SESSION_SECURE_COOKIE=true
SESSION_HTTP_ONLY=true
SESSION_SAME_SITE=strict

BROADCAST_CONNECTION=log
FILESYSTEM_DISK=local
QUEUE_CONNECTION=database

CACHE_STORE=database
CACHE_PREFIX=

MEMCACHED_HOST=127.0.0.1

REDIS_CLIENT=predis
REDIS_HOST=127.0.0.1
REDIS_PASSWORD=null
REDIS_PORT=6379

MAIL_MAILER=log
MAIL_SCHEME=null
MAIL_HOST=127.0.0.1
MAIL_PORT=2525
MAIL_USERNAME=null
MAIL_PASSWORD=null
MAIL_FROM_ADDRESS="hello@example.com"
MAIL_FROM_NAME="${APP_NAME}"

# Workflow — Email Evidence (Phase 0/1)
# link_only | snapshot | both
EMAIL_EVIDENCE_MODE=both
# Max size for a single attachment snapshot in bytes (default 25MB)
EMAIL_EVIDENCE_MAX_ATTACHMENT=26214400
# Domain used to generate forward-to-ingest addresses (e.g., wf+<id>@domain)
EMAIL_INGEST_DOMAIN=example.com

# OAuth — Gmail (Phase 1)
GOOGLE_CLIENT_ID=
GOOGLE_CLIENT_SECRET=
# Must match your Google console OAuth redirect URI
GOOGLE_REDIRECT_URI=${APP_URL}/api/email-accounts/callback/google
# Space-delimited scopes; keep minimal
GOOGLE_SCOPES=https://www.googleapis.com/auth/gmail.readonly offline_access

# OAuth — Microsoft Graph (Outlook) (Phase 1)
MICROSOFT_CLIENT_ID=
MICROSOFT_CLIENT_SECRET=
MICROSOFT_TENANT_ID=common
# Must match Azure app redirect URI
MICROSOFT_REDIRECT_URI=${APP_URL}/api/email-accounts/callback/outlook
# Space-delimited scopes; keep minimal
MICROSOFT_SCOPES=Mail.Read offline_access

# Watchers (Phase 2 — optional)
GMAIL_PUBSUB_PROJECT_ID=
GMAIL_PUBSUB_TOPIC=
GRAPH_SUBSCRIPTION_NOTIFY_URL=${APP_URL}/api/email-accounts/notify/outlook

AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_DEFAULT_REGION=us-east-1
AWS_BUCKET=
AWS_USE_PATH_STYLE_ENDPOINT=false

VITE_APP_NAME="${APP_NAME}"

JWT_SECRET=
SANCTUM_STATEFUL_DOMAINS=localhost,localhost:3000,127.0.0.1,127.0.0.1:8000
SANCTUM_EXPIRATION=43200
FORCE_HTTPS=false

#sizes media
images=2048000
gallery=2048000
documents=2048000
videos=20480000
logo=20480000
avatar=20480000
default=2048000
#sizes media


#/* real time */
PORT_NOTIFICATIONS=8890
#HTTPS VALUES MUST BE TRUE OR FALSE
HTTPS=FALSE
SSL_KEY=privkey.pem
SSL_CERT=fullchain.pem
SSL_CA=chain.pem
#/* real time */
